{
	"name": "tiny-encrypt",
	"version": "0.0.0-development",
	"description": "Tiny string encryption and comparison library with both async and sync support",
	"main": "dist/index.js",
	"homepage": "https://github.com/seanwlawrence//tiny-encrypt#readme",
	"bugs": {
		"url": "https://github.com/seanWLawrence/tiny-encrypt/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/seanWLawrence/tiny-encrypt.git"
	},
	"scripts": {
		"lint": "tslint -c tslint.json 'src/**/*.ts'",
		"format": "tsfmt -r",
		"test": "jest",
		"test:watch": "jest --watch",
		"develop": "nodemon",
		"build": "tsc",
		"commit": "npm run build && npx git-cz",
		"ci": "npm run lint && npm run test && npm publish",
		"publish": "semantic-release"
	},
	"keywords": [
		"encrypt",
		"encryption",
		"decrypt",
		"decryption",
		"crypto",
		"hash",
		"password"
	],
	"author": "Sean W. Lawrence",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^23.3.2",
		"@types/node": "^10.10.1",
		"commitizen": "^2.10.1",
		"coveralls": "^3.0.2",
		"cz-conventional-changelog": "^2.1.0",
		"jest": "^23.6.0",
		"nodemon": "^1.18.4",
		"semantic-release": "^15.9.16",
		"travis-deploy-once": "^5.0.8",
		"ts-jest": "^23.10.0",
		"ts-node": "^7.0.1",
		"tslint": "^5.11.0",
		"typescript": "^3.0.3",
		"typescript-formatter": "^7.2.2"
	},
	"dependencies": {
		"cross-fetch": "^2.2.2",
		"express": "^4.16.3"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-conventional-changelog"
		}
	}
}
